{
  "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-object-rest-spread",
      ["module-resolver", {
        "root": ["./"],
        "alias": {
          "app": "./app"
        }
      }],
    [
      "import",
        [{
          "libraryName": "antd-mobile",
          "style": "css"
        }]
      ]
    ]
}
